                     , directory
                     , filepath
                     , unix
                     , blaze-html
                     , blaze-markup
                     , clay
                     , text
                     , split
                     , markdown
                     , pureMD5
                     , bytestring
  hs-source-dirs:      src
  default-language:    Haskell2010
